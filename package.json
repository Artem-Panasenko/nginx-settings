{
  "name": "nginxsettings",
  "version": "0.0.1",
  "readme": "README.md",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "6.3.26",
    "babel-polyfill": "6.3.14",
    "babel-preset-es2015": "6.1.18",
    "babel-register": "6.24.1",
    "body-parser": "^1.13.3",
    "express": "^4.15.2",
    "express-jwt": "^3.0.0",
    "express-session": "^1.11.3",
    "method-override": "^2.3.5",
    "morgan": "~1.7.0",
    "request": "^2.81.0"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=5.2"
  },
  "scripts": {
    "build": "NODE_ENV=development webpack",
    "build:prod": "NODE_ENV=production webpack",
    "build:test": "NODE_ENV=test webpack",
    "serve": "NODE_ENV=development webpack --watch --progress --colors",
    "translations:clone": "git clone git@github.private.linksynergy.com:SD/reporting-translations.git ./translations",
    "translations:copy": "cp ./translations/reporting/* ./app/scripts/i18n",
    "translations:remove": "rm -rf translations",
    "trans": "npm run translations:clone && npm run translations:copy && npm run translations:remove",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose-dev.yml up -d",
    "dev": "npm run docker:dev && npm run serve"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
